DELIMITER $$
CREATE DEFINER=`root`@`localhost` PROCEDURE `FanPinStatusUpdation`(in fp varchar(25),in st varchar(25),in bno int)
BEGIN
declare i integer;
declare exit handler for sqlwarning
	begin
		select 'error occured';
		rollback;
	end;
		start transaction;
		select StatusId into i from iocl_supported_pinstatus where FanPinStatus=st;
		update iocl_fanslip_details set FanPinStatusId=i where FanPin=fp and BayNo=bno;
	commit;
END$$
DELIMITER ;
